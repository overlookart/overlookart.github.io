{
    "asciidocExt": {},
    "defaultMarkdownHandler": "goldmark",
    "goldmark": {
        "extensions": {
            "strikethrough": true,
            "definitionList": true,
            "linkify": true,
            "table": true
        },
        "parser": {
            "wrapStandAloneImageWithinParagraph": true,
            "attribute": {
                "block": true
            }
        },
        "renderer": {
            "hardWraps": true,
            "unsafe": true,
            "xhtml": true
        }
    },
    "highlight": {
        "codeFences": true,
        "style": "rose-pine-dawn",
        "noClasses":true,
        "guessSyntax": false,
        "lineNos": true,
        "lineNoStart": 1,
        "lineNumbersInTable": false,
        "noHl": false,
        "hl_Lines": "",
        "hl_inline": false,
        "anchorLineNos": false,
        "lineAnchors": "",
        "tabWidth": 4
    },
    "tableOfContents": {
        "startLevel": 2,
        "endLevel": 4,
        "ordered": true
    }
}